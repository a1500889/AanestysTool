
public class Kirja {
	public static String nimi;
	public static String tekijatiedot;
	public static String ISBN;
	public Kirja() {
		super();
		// TODO Auto-generated constructor stub
	}
	public static String getNimi() {
		return nimi;
	}
	public static void setNimi(String nimi) {
		Kirja.nimi = nimi;
	}
	public static String getTekijatiedot() {
		return tekijatiedot;
	}
	public static void setTekijatiedot(String tekijatiedot) {
		Kirja.tekijatiedot = tekijatiedot;
	}
	public static String getISBN() {
		return ISBN;
	}
	public static void setISBN(String iSBN) {
		ISBN = iSBN;
	}

}


